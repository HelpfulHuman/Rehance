{
  "name": "rehance",
  "version": "1.0.2",
  "description": "Utilities for rapid-generation of stateful React components.",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push origin master && git push origin master --tags",
    "clean": "rm -rf dist/",
    "build": "rollup -c",
    "watch": "npm run build -- --watch",
    "test": "mocha",
    "tdd": "npm run watch | npm run test -- --watch",
    "prepublish": "npm run clean && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpfulhuman/renhance.git"
  },
  "keywords": [
    "react",
    "utilities",
    "enhance",
    "enhancement"
  ],
  "author": "Nick Glenn <nick@helpfulhuman.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helpfulhuman/renhance/issues"
  },
  "homepage": "https://github.com/helpfulhuman/renhance#readme",
  "dependencies": {
    "react": "^16.1.0"
  },
  "devDependencies": {
    "@types/react": "^16.0.38",
    "jest": "^22.3.0",
    "rollup": "^0.51.8",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-typescript2": "^0.8.4",
    "rollup-watch": "^4.3.1",
    "ts-jest": "^22.0.4",
    "typescript": "^2.7.2"
  }
}
